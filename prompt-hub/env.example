# WebSocket configuration for prompt watcher
# Hostname for WebSocket connections (defaults to window.location.hostname)
NEXT_PUBLIC_WS_HOST=localhost

# Port for WebSocket connections (defaults to 3001)
NEXT_PUBLIC_WS_PORT=3001

# Enable watcher in development mode (set to "true" to enable)
NEXT_PUBLIC_ENABLE_WATCHER=false

# WebSocket server port for the production watcher
WS_PORT=3001

